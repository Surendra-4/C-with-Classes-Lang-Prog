#include <iostream>
using namespace std;

int main(){
    int a,b;
    cout<<"Enter the first number : ";
    cin>>a;
    cout<<"Enter the second number : ";
    cin>>b;
    
    if(a>b){
        cout<<"The largest number is a = "<<a<<endl;
    }
    
    if(b>a){
        cout<<"The largest number is b = "<<b<<endl;
    }
    return 0;
}
